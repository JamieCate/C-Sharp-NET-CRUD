@{
    ViewData["Title"] = "CMS";
}

@model VertoTest.Models.TopHomeImageTextModel




<h1 class="text-center">@ViewData["Title"]</h1>

<form method="post">


    <div class="container">

        <h4 class="text-center">Edit Home Image Text</h4>
        <label class="form-label">Title</label>
        <input type="text" class="form-control" asp-for="ImageTitle" />

        <h4 class="text-center">Edit Home Image Description</h4>
        <label class="form-label">Descritption</label>
        <input type="text" class="form-control" asp-for="ImageDescription" />

            <button type="submit" class="btn btn-primary">Save</button>

          

    </div>

</form>